<!DOCTYPE html>
<html lang="en" dir="ltr">
<%- include('../partials/head.ejs') %>
  <body>
    <%- include('../partials/header.ejs') %>
      <div class="show-container">
        <div class="show-img-container">
          <img class="show-img" src="<%=photos.img%>" alt="<%=photos.photographer%>">
        </div>
        <div class="show-info">
          <h2><%=photos.photographer%></h2>
          <h4>Price: $<%=photos.price%></h4>
          <h4><%=photos.description %></h4>
          <h4>Qty: <%=photos.qty%></h4>
             <a href="/photos/<%=photos.id%>/edit">Edit</a>
            <form action="/photos/<%=photos.id%>?_method=DELETE" method="POST">
              <input type="submit" name="" value="DELETE">
            </form>
        </div>
       </div>
  </body>
</html>
